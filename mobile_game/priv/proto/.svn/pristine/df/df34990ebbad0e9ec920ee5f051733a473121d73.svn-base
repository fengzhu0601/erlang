## 属性
struct attr 
{
    attId          : u8, ## non-used
    attHp          : u32,
    attMp          : u32,
    attSp          : u32, ## 废弃可以删除
    attNp          : u32, ## 能量
    attStrength    : u32, ## 力量
    attIntellect   : u32, ## 智力
    attNimble      : u32, ## 敏捷
    attStrong      : u32, ## 体质
    attAtk         : u32, ## 攻击
    attDef         : u32, ## 防御
    attCrit        : u32, ## 暴击等级
    attBlock       : u32, ## 格挡
    attPliable     : u32, ## 柔韧
    attPure_atk    : u32, ## 无视防御伤害
    attBreak_def   : u32, ## 破甲
    attAtk_deep    : u32, ## 伤害加深
    attAtk_free    : u32, ## 伤害减免
    attAtk_speed   : u32, ## 攻击速度
    attPrecise     : u32, ## 精确
    attThunder_atk : u32, ## 雷攻
    attThunder_def : u32, ## 雷防
    attFire_atk    : u32, ## 火
    attFire_def    : u32,
    attIce_atk     : u32, # 冰
    attIce_def     : u32,
    attMove_speed  : u32, # 移动速度
    attRun_speed   : u32,  # 跑步速度
	attSuckBlood   : u32, #吸血
	attReverse	   : u32, #反伤
	attBati		   : u32  #霸体
}

struct item_info
{
    Id:item_id,       #物品id
    Bid:item_bid,     #物品bid
    Pos:item_pos,     #物品位置
    Qly:u8,           #物品品质
    Qua:item_count,   #物品数量
    Bind:bool,        #物品绑定状态 0非绑 1绑定

    IsJd:bool,        #是否鉴定
    SuitId:u32,       #套装id
    QhLev:u8,         #强化等级
    Power:u32,        #装备评分
    
    ExtraAttr:list<{
        ExtraModId:u8,   #属性所属模块 1鉴定属性
        Attr:list<{
            AttrCode:u8, #属性代号
            AttrVal:u32, #属性值
            AttrPer:u8   #随机属性值对于满属性的百分比1-100
        } >
    }>,
    GemInfo:list<GemId:item_bid>,
    ItemEx:list<{Key:u8,Val:u32}>,
    SkillChanges:list<Id:u32>,   #技能修改集
    JDListMax:list<Max:u32>,
    JDListMin:list<Min:u32>
}

