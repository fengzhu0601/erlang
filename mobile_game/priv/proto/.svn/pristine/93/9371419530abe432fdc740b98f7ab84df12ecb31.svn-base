@include "game.sproto"



# 请求进入场景,
# SceneId:16
proto MSG_SCENE_PLAYER_ENTER_REQUEST=1
{
    client{
        message{
            sceneId:scene_id
        }
    }

    server {
        message{
            sceneId:scene_id
            ,posX:u16 #进入点x
            ,posY:u16 #进入点y
            ,dir:u8
        }
    }
}


# 进入场景
proto MSG_SCENE_PLAYER_ENTER=2
{
    client{
        message=empty
    }

    server{
        message{
            idx:agent_id ##
       }
    }
}

